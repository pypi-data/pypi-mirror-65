{
  "name": "sn-jupyterlab-github",
  "version": "1.1.8",
  "description": "JupyterLab viewer for GitHub repositories",
  "keywords": [
    "github",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.ibm.com/skills-network-labs/jupyterlab-github",
  "bugs": {
    "url": "https://github.ibm.com/skills-network-labs/jupyterlab-github/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/skills-network-labs/jupyterlab-github"
  },
  "license": "BSD-3-Clause",
  "author": "Ian Rose (and modified by James Reeve)",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json",
    "style/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc",
    "build:test": "cd test && ./build-tests.sh",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "test": "cd test && ./run-tests.sh",
    "watch": "tsc -w"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/docmanager": "^1.0.0",
    "@jupyterlab/docregistry": "^1.0.0",
    "@jupyterlab/filebrowser": "^1.0.0",
    "@jupyterlab/services": "^4.0.0",
    "@phosphor/algorithm": "^1.1.3",
    "@phosphor/messaging": "^1.2.3",
    "@phosphor/signaling": "^1.2.3",
    "@phosphor/widgets": "^1.8.0",
    "base64-js": "^1.3.0"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/text-encoding": "^0.0.35",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2"
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "sn_jupyterlab_github"
        }
      }
    },
    "schemaDir": "schema"
  }
}
