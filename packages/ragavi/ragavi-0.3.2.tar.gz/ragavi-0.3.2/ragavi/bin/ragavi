#!/usr/bin/env python3.6

from ragavi.arguments import gains_argparser
from ragavi.utils import logger

logger.warn("""Use of 'ragavi' will be deprecated in future; Use 'ragavi-gains' instead to access the gains plotter.""")

parser = gains_argparser()
options = parser.parse_args()

if __name__ == "__main__":
    from ragavi.ragavi import main
    main(options=options)
